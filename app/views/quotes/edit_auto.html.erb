<%= form_for @quote_auto do |f| %>
<div class="large-6 large-centered">
    <div class='large-4 large-centered text-center'>
        <h3>Quotes Auto</h3>
    </div>
    <%= @quote_auto.errors.full_messages.first if @quote_auto.errors.any? %>
    <div class="small-12 collapse columns">
        <label>Promo Date</label>
        <%= f.text_field :promo_date, placeholder: "Promo Date" %>
    </div>
    <div class="small-12 collapse columns">
        <label>Category</label>
        <%= f.text_field :category, placeholder: "Category" %>
    </div>
    <div class="small-12 collapse columns">
        <label>Carrier</label>
        <%= f.text_field :carrier, placeholder: "Carrier" %>
    </div>
    <div class="small-12 collapse columns">
        <label>Bodily Injury Liability</label>
        <%= f.text_field :bodily_injury_liability, placeholder: "eg. $15,000 / PP   $30,000 / OCC" %>
    </div>
    <div class="small-12 collapse columns">
        <label>Property Damage</label>
        <%= f.text_field :property_damage_liability, placeholder: "eg. $10,000" %>
    </div>
    <div class="small-12 collapse columns">
        <label>Medical Payments</label>
        <%= f.text_field :medical_payments, placeholder: "eg. $1,000" %>
    </div>
    <div class="small-12 collapse columns">
        <label>Unisured Motorist Bodily Injury</label>
        <%= f.text_field :uninsured_motorist_bodily_injury, placeholder: "eg. $15,000 / PP  $30,000 / OCC" %>
    </div>
    <div class="small-12 collapse columns">
        <label>Comprehensive</label>
        <%= f.text_field :comprehensive, placeholder: "eg. Not Included or $500" %>
    </div>
    <div class="small-12 collapse columns">
        <label>Collision</label>
        <%= f.text_field :collision, placeholder: "eg. Not Included or $500" %>
    </div>
    <div class="small-12 collapse columns">
        <label>Waiver of Collision Deductible</label>
        <%= f.text_field :waiver_of_collision_deductible, placeholder: "eg. Included or $3,500" %>
    </div>
    <div class="small-12 collapse columns">
        <label>Rental Reimbursement</label>
        <%= f.text_field :rental_reimbursement, placeholder: "eg. Included or $30 day for 30 days" %>
    </div>
    <div class="small-12 collapse columns">
        <label>Roadside Coverage</label>
        <%= f.select :roadside_coverage, [['Select Roadside Coverage', ''], ['Y', 'Y'], ['N', 'N']] %>
    </div>
    <div class="small-12 collapse columns">
        <label>Total Premium</label>
        <%= f.text_field :total_premium, placeholder: "$600 / Month" %>
    </div>
    <div class="small-12 columns">
        <button class="button">
            Submit
        </button>
    </div>
</div>
<% end %>
